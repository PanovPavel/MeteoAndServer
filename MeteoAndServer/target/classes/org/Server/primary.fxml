<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="TOP_CENTER" prefHeight="276.0" prefWidth="405.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.Server.PrimaryController">
   <children>
      <HBox prefHeight="51.0" prefWidth="405.0">
         <children>
            <Button fx:id="ServerStart" mnemonicParsing="false" onAction="#switchToStartServer" text="StartServer">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </Button>
            <Button fx:id="StopServer" contentDisplay="CENTER" mnemonicParsing="false" onAction="#switchToStopServer" text="StopServer" />
         </children>
         <VBox.margin>
            <Insets bottom="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <Label fx:id="informationFromServer" prefHeight="275.0" prefWidth="405.0" text="Label" />
   </children>
</VBox>
